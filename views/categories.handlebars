{{#each tags as |tag|}}
<div class="row mb-4 blogpost">
  <div class="col-md-5">
    <h2>
      <a href="/tag/{{tag.id}}">{{tag.tag_name}}</a>
    </h2>
    {{#each tag.codesnips as |snode|}}
    <div>
        <a href="/snode/{{snode.id}}"><p>{{snode.description}}</p></a>
    </div>
    {{/each}}
  </div>
  <div class="col-md-7">
    <p>
      {{snode.content}}
    </p>
  </div>
</div>
{{/each}}