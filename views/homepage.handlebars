<!-- Loop over each object in the snodes array -->
{{#each snodes as |snode|}}
<div class="row mb-4 blogpost">
  <div class="col-md-5">
    <h2>{{snode.description}}</a></h2>
    <p>Written by <a href="/profile/{{snode.user.id}}">{{snode.user.name}}</a> on {{snode.date}}</p>
  </div>
  <div class="col-md-7">
    <pre>{{snode.content}}</pre>
  </div>
</div>
{{/each}}
