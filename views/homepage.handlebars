<!-- Loop over each object in the blogpost array -->
{{!-- {{#if logged_in}} --}}
{{#each snodes as |snode|}}
<div class="row mb-4 blogpost">
  <div class="col-md-5">
    <h2>
      <a href="/blogpost/{{snode.id}}">{{snode.description}}</a>
    </h2>
    <p>Written by {{snode.user_id}} on {{format_date snode.date}}</p>
  </div>
  <div class="col-md-7">
    <p>
      {{snode.content}}
    </p>
  </div>
</div>
{{/each}}
{{!-- {{/if}} --}}